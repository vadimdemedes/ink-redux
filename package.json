{
	"name": "ink-redux",
	"version": "1.0.0",
	"description": "Redux bindings for Ink",
	"license": "MIT",
	"repository": "vadimdemedes/ink-redux",
	"author": {
		"name": "Vadim Demedes",
		"email": "vdemedes@gmail.com",
		"url": "github.com/vadimdemedes"
	},
	"main": "dist/index.js",
	"types": "index.d.ts",
	"engines": {
		"node": ">=6"
	},
	"scripts": {
		"pretest": "npm run build",
		"test": "xo && ava",
		"build": "babel src --out-dir=dist",
		"prepublish": "npm run build"
	},
	"files": [
		"dist",
		"index.d.ts"
	],
	"keywords": [
		"ink",
		"redux"
	],
	"dependencies": {
		"@types/react-redux": "^4.4.45"
	},
	"devDependencies": {
		"ava": "^0.20.0",
		"babel-cli": "^6.24.1",
		"babel-plugin-transform-react-jsx": "^6.24.1",
		"eslint-config-xo-react": "^0.13.0",
		"eslint-plugin-react": "^7.1.0",
		"ink": "^0.1.2",
		"redux": "^3.7.1",
		"sinon": "^2.3.6",
		"strip-ansi": "^4.0.0",
		"xo": "^0.18.2"
	},
	"babel": {
		"presets": [
			"@ava/stage-4"
		],
		"plugins": [
			[
				"transform-react-jsx",
				{
					"pragma": "h",
					"useBuiltIns": true
				}
			]
		]
	},
	"ava": {
		"babel": {
			"plugins": [
				[
					"transform-react-jsx",
					{
						"pragma": "h",
						"useBuiltIns": true
					}
				]
			]
		}
	},
	"xo": {
		"extends": [
			"xo-react"
		],
		"rules": {
			"react/prop-types": 0
		},
		"settings": {
			"react": {
				"pragma": "h"
			}
		}
	}
}
